{
    "name": "bag2/string-generator",
    "description": "String Generator",
    "license": "Apache-2.0",
    "require": {
        "php": ">= 7.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^8.5|^7.5|^4.8",
        "symplify/easy-coding-standard": "^12.0",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "autoload": {
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Bag2\\string_generator\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
